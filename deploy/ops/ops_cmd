dlv exec "./bin/manager"

# 加密例子
echo zhanggong | openssl aes-256-cbc -K 123123 -base64 -iv 123123 -A

# 解密例子
echo xxx== | openssl aes-256-cbc -d -K 123123 -base64 -iv 123123 -A


# MySQL连接权限
GRANT SHOW DATABASES, PROCESS, REPLICATION SLAVE, REPLICATION CLIENT ON *.* TO 'xxx'@'%' identified by '密码';
GRANT SELECT ON *.* TO 'xxx'@'%';

